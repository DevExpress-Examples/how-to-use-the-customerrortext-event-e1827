      <assemblies>
        <add assembly="DevExpress.Xpo.v15.2, Version=15.2.0.0, Culture=neutral, PublicKeyToken=79868B8147B5EAE4" />
        <add assembly="DevExpress.Web.ASPxScheduler.v15.2, Version=15.2.0.0, Culture=neutral, PublicKeyToken=79868B8147B5EAE4" />
        <add assembly="DevExpress.Web.v15.2, Version=15.2.0.0, Culture=neutral, PublicKeyToken=79868B8147B5EAE4" />
        <add assembly="DevExpress.Printing.v15.2.Core, Version=15.2.0.0, Culture=neutral, PublicKeyToken=79868B8147B5EAE4" />
